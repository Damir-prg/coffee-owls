{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:js": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" --config ./.eslintrc",
    "lint:style": "stylelint \"src/**/*.{css,scss}\" --config ./.stylelintrc",
    "lint": "npm run lint:js && npm run lint:style",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\" --config ./.prettierrc",
    "test": "jest ./"
  },
  "dependencies": {
    "antd": "^5.16.2",
    "dotenv": "^16.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^28",
    "jest-environment-jsdom": "^29.0.1",
    "lefthook": "^1.3.9",
    "prettier": "^3.2.5",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-scss": "^6.2.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2",
    "vite": "^3.0.7",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "license": "MIT"
}
